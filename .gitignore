# Ignore node_modules for both frontend and backend
node_modules/
frontend/node_modules/
backend/node_modules/

# Ignore build output
build/
dist/

# Ignore environment variables and secrets
.env
frontend/.env
backend/.env

# Ignore log files
*.log
frontend/*.log
backend/*.log
logs/
frontend/logs/
backend/logs/

# Ignore system files
.DS_Store
Thumbs.db

# Ignore temporary files and folders
tmp/
temp/
*.tmp
*.swp

# Ignore cache files
.cache/
frontend/.cache/
backend/.cache/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore coverage output
coverage/
frontend/coverage/
backend/coverage/

# Ignore IDE and editor configuration
.vscode/
.idea/

# Ignore transpiled and static assets
public/static/
frontend/public/static/
backend/public/static/

# Ignore compiled code (if using TypeScript in any part)
*.tsbuildinfo
frontend/*.tsbuildinfo
backend/*.tsbuildinfo
